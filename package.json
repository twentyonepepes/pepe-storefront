{
  "name": "ecommerce-app-client",
  "main": "index.jsx",
  "scripts": {
    "build": "babel src/server -d bin/server",
    "build.container": "docker build . -t pepe-store/storefront -t pepestoreacr.azurecr.io/pepe-store/storefront",
    "test": "npm run build && cross-env IMAGE_SERVER_URL=http://localhost:7781 PRODUCT_SERVER_URL=http://localhost:7777 NODE_ENV=development node bin/server",
    "test.container": "docker context use default && npm run build.container && npm run test.deploy",
    "test.deploy":"docker run -p 80:80 --name=storefront --network=pepe -d pepe-store/storefront",
    "prepush": "npm run build.container",
    "push": "docker push pepestoreacr.azurecr.io/pepe-store/storefront",
    "start": "node bin/server",
    "webpack": "parallel-webpack"
  },
  "devDependencies": {
